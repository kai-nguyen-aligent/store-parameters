{
  "name": "store-parameters",
  "description": "CLI tool for import and export SSM parameters",
  "version": "1.1.0",
  "author": "Kai Nguyen",
  "bin": {
    "store-parameters": "./bin/run.js"
  },
  "bugs": "https://github.com/kai-nguyen-aligent/store-parameters/issues",
  "dependencies": {
    "@1password/op-js": "^0.1.13",
    "@aws-sdk/client-ssm": "^3.864.0",
    "@aws-sdk/credential-providers": "^3.864.0",
    "@inquirer/prompts": "^5.5.0",
    "@json2csv/formatters": "^7.0.6",
    "@json2csv/node": "^7.0.6",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "arktype": "^2.1.22",
    "chalk": "^5.4.1",
    "csvtojson": "^2.0.10"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kai-nguyen-aligent/store-parameters",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "store-parameters",
    "dirname": "store-parameters",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kai-nguyen-aligent/store-parameters",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6"
}
